#1
SELECT first_name, last_name, email FROM  person_detail WHERE first_name = "Emilie" AND last_name ="Mikešová";

#2
SELECT user_id, name FROM user WHERE NAME LIKE "k%" AND user_id BETWEEN 250 AND 700;
