#1
SELECT *  FROM uzivatele WHERE prezdivka = "Denny";

#2
SELECT * FROM uzivatele WHERE uzivatele_id >=2 AND uzivatele_id<=4;

#3
SELECT * FROM uzivatele WHERE prezdivka LIKE "%a" OR prezdivka LIKE "e__";

